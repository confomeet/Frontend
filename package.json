  "scripts": {
    "git-info": "echo  \"{\\\"short-commit\\\": \\\"$(git log -1 --oneline)\\\",\\\"long-commit\\\": \\\"$(git rev-parse --verify HEAD)\\\",\\\"branch-tag\\\": \\\"$( git tag -l -n10 $(git describe))\\\", \\\"branch\\\": \\\"$(git rev-parse --abbrev-ref HEAD)\\\", \\\"date\\\": \\\"$(git log -1 --format=%ct)\\\"}\"  > src/release_meatdata.json",
    "create-certs": "openssl req -x509 -newkey rsa:4096 -keyout build/key.pem -out build/cert.pem -sha256 -days 3650 -nodes -subj '/C=XX/ST=StateName/L=CityName/O=CompanyName/OU=CompanySectionName/CN=localhost'",
    "start-ssl": "HTTPS=true SSL_CRT_FILE=build/cert.pem SSL_KEY_FILE=build/key.pem react-scripts start",
    "start": "react-scripts start",
    "build": "yarn git-info && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
